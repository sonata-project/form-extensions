{
    "name": "sonata-project/form-extensions",
    "type": "symfony-bundle",
    "description": "Symfony form extensions",
    "keywords": [
        "symfony",
        "form"
    ],
    "homepage": "https://sonata-project.org/bundles/form-extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/form-extensions/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/form": "^2.8 || ^3.2 || ^4.0",
        "symfony/translation": "^2.8 || ^3.2 || ^4.0",
        "symfony/validator": "^2.8 || ^3.2 || ^4.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "^2.8 || ^3.3 || ^4.0",
        "symfony/expression-language": "^2.8 || ^3.3 || ^4.0",
        "symfony/framework-bundle": "^2.8 || ^3.3 || ^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/twig-bridge": "^2.8 || ^3.4 || ^4.0",
        "symfony/twig-bundle": "^2.8 || ^3.4 || ^4.0"
    },
    "suggest": {
        "symfony/framework-bundle": "If you want to use symfony"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\Form\\Tests\\": "tests/"
        }
    }
}
