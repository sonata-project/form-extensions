{
    "name": "sonata-project/form-extensions",
    "type": "symfony-bundle",
    "description": "Symfony form extensions",
    "keywords": [
        "symfony",
        "form"
    ],
    "homepage": "https://sonata-project.org/bundles/form-extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/form-extensions/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/event-dispatcher": "^3.4 || ^4.0",
        "symfony/form": "^3.4 || ^4.0",
        "symfony/options-resolver": "^3.4 || ^4.0",
        "symfony/property-access": "^3.4 || ^4.0",
        "symfony/security-csrf": "^3.4 || ^4.0",
        "symfony/translation": "^3.4 || ^4.0",
        "symfony/validator": "^3.4 || ^4.0",
        "twig/twig": "^2.0"
    },
    "conflict": {
        "sonata-project/core-bundle": ">=3.13"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^3.0",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/twig-bridge": "^3.4 || ^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\Form\\Tests\\": "tests/"
        }
    }
}
